<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#333b6a"
    android:onClick="SignOut"
    android:orientation="vertical"
    tools:context=".controllers.UserProfileActivity">

  <ImageView
      android:id="@+id/imageView4"
      android:layout_width="53dp"
      android:layout_height="53dp"
      android:layout_marginStart="16dp"
      android:layout_marginTop="13dp"
      android:onClick="MainActivity"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/return_icon" />

  <Button
      android:id="@+id/reserve"
      android:layout_width="118dp"
      android:layout_height="58dp"
      android:layout_marginStart="148dp"
      android:layout_marginTop="472dp"
      android:backgroundTint="#8DE3CA"
      android:onClick="SignOut"
      android:text="Logout"
      android:textColor="#333b6a"
      android:textSize="16sp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <TextView
      android:id="@+id/user_info"
      android:layout_width="267dp"
      android:layout_height="176dp"
      android:layout_marginStart="72dp"
      android:layout_marginTop="236dp"
      android:fontFamily="sans-serif-medium"
      android:textColor="#FAF6F6"
      android:textColorHighlight="#FAF6F6"
      android:textColorHint="#FAF6F6"
      android:textColorLink="#FAF6F6"
      android:textSize="20sp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <TextView
      android:id="@+id/textView6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="140dp"
      android:fontFamily="sans-serif-black"
      android:text="User Info"
      android:textColor="#E6E8E7"
      android:textSize="30sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>