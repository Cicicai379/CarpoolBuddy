<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/p"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#333b6a"
    android:onClick="SelectImage"
    android:orientation="vertical"
    tools:context=".controllers.AddVehicleActivity">


    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="104dp"
        android:fontFamily="sans-serif-black"
        android:text="Ride Details"
        android:textColor="#E6E8E7"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/reserve"
        android:layout_width="109dp"
        android:layout_height="49dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="508dp"
        android:backgroundTint="#8DE3CA"
        android:onClick="reserve"
        android:text="Reserve"
        android:textColor="#333b6a"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="53dp"
        android:layout_height="53dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="13dp"
        android:onClick="MainActivity"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/return_icon" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="149dp"
        android:layout_height="150dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="128dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_dialog_info" />

    <TextView
        android:id="@+id/signup_password"
        android:layout_width="239dp"
        android:layout_height="20dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="376dp"
        android:fontFamily="sans-serif-medium"
        android:textColor="#F8F7F7"
        android:textColorHighlight="#F8F7F7"
        android:textColorHint="#F8F7F7"
        android:textColorLink="#F8F7F7"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/profile_type"
        android:layout_width="254dp"
        android:layout_height="24dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="436dp"
        android:fontFamily="sans-serif-medium"
        android:textColor="#F8F7F7"
        android:textColorHighlight="#F8F7F7"
        android:textColorHint="#F8F7F7"
        android:textColorLink="#F8F7F7"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/profile_contact"
        android:layout_width="253dp"
        android:layout_height="21dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="460dp"
        android:fontFamily="sans-serif-medium"
        android:textColor="#F8F7F7"
        android:textColorHighlight="#F8F7F7"
        android:textColorHint="#F8F7F7"
        android:textColorLink="#F8F7F7"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/profile_price"
        android:layout_width="245dp"
        android:layout_height="20dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="416dp"
        android:fontFamily="sans-serif-medium"
        android:textColor="#F8F7F7"
        android:textColorHighlight="#F8F7F7"
        android:textColorHint="#F8F7F7"
        android:textColorLink="#F8F7F7"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/signup_email"
        android:layout_width="238dp"
        android:layout_height="20dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="356dp"
        android:fontFamily="sans-serif-medium"
        android:textColor="#F8F7F7"
        android:textColorHighlight="#F8F7F7"
        android:textColorHint="#F8F7F7"
        android:textColorLink="#F8F7F7"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/profile_capacity"
        android:layout_width="242dp"
        android:layout_height="25dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="396dp"
        android:fontFamily="sans-serif-medium"
        android:textColor="#F8F7F7"
        android:textColorHighlight="#F8F7F7"
        android:textColorHint="#F8F7F7"
        android:textColorLink="#F8F7F7"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>